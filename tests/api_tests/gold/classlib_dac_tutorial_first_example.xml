<?xml version="1.0" encoding="UTF-8"?>
<pss:model 
  xmlns:pss="http://accellera.org/PSS"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://accellera.org/PSS PSSModel.xsd">
    <pss:component name="C">
        <pss:action name="A">
            <pss:field name="field1" qualifier="rand">
                <pss:type>
                    <pss:bit>
                        <pss:msb>
                            <pss:number>0x3</pss:number>
                        </pss:msb>
                        <pss:lsb>
                            <pss:number>0x0</pss:number>
                        </pss:lsb>
                    </pss:bit>
                </pss:type>
            </pss:field>
        </pss:action>
    </pss:component>
    <pss:component name="static_structure">
        <pss:field name="c1">
            <pss:type>
                <pss:user>
                    <pss:path>C</pss:path>
                </pss:user>
            </pss:type>
        </pss:field>
        <pss:field name="c2">
            <pss:type>
                <pss:user>
                    <pss:path>C</pss:path>
                </pss:user>
            </pss:type>
        </pss:field>
        <pss:action name="entry">
            <pss:field name="a1">
                <pss:type>
                    <pss:user>
                        <pss:path>C</pss:path>
                        <pss:path>A</pss:path>
                    </pss:user>
                </pss:type>
            </pss:field>
            <pss:field name="a2">
                <pss:type>
                    <pss:user>
                        <pss:path>C</pss:path>
                        <pss:path>A</pss:path>
                    </pss:user>
                </pss:type>
            </pss:field>
            <pss:graph>
                <pss:repeat type="count">
                    <pss:expr>
                        <pss:number>0x5</pss:number>
                    </pss:expr>
                    <pss:body>
                        <pss:traverse name="a1"/>
                        <pss:traverse name="a2">
                            <pss:with>
                                <pss:stmt>
                                    <pss:binexp op="LT">
                                        <pss:lhs>
                                            <pss:ref>
                                                <pss:path>a2</pss:path>
                                                <pss:path>field1</pss:path>
                                            </pss:ref>
                                        </pss:lhs>
                                        <pss:rhs>
                                            <pss:number>0x2</pss:number>
                                        </pss:rhs>
                                    </pss:binexp>
                                </pss:stmt>
                            </pss:with>
                        </pss:traverse>
                    </pss:body>
                </pss:repeat>
            </pss:graph>
        </pss:action>
    </pss:component>
</pss:model>
