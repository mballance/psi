<?xml version="1.0" encoding="UTF-8"?>
<pss:model 
  xmlns:pss="http://accellera.org/PSS"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://accellera.org/PSS PSSModel.xsd">
    <pss:package>
        <pss:struct name="constraint_basics_test">
            <pss:field name="address" qualifier="rand">
                <pss:type>
                    <pss:bit>
                        <pss:msb>
                            <pss:number>31</pss:number>
                        </pss:msb>
                        <pss:lsb>
                            <pss:number>0</pss:number>
                        </pss:lsb>
                    </pss:bit>
                </pss:type>
            </pss:field>
            <pss:constraint>
                <pss:stmt>
                    <pss:binexp op="GE">
                        <pss:lhs>
                            <pss:ref>
                                <pss:path>address</pss:path>
                            </pss:ref>
                        </pss:lhs>
                        <pss:rhs>
                            <pss:number>0x1000</pss:number>
                        </pss:rhs>
                    </pss:binexp>
                </pss:stmt>
            </pss:constraint>
            <pss:constraint>
                <pss:stmt>
                    <pss:binexp op="AndAnd">
                        <pss:lhs>
                            <pss:binexp op="GE">
                                <pss:lhs>
                                    <pss:ref>
                                        <pss:path>address</pss:path>
                                    </pss:ref>
                                </pss:lhs>
                                <pss:rhs>
                                    <pss:number>0x1000</pss:number>
                                </pss:rhs>
                            </pss:binexp>
                        </pss:lhs>
                        <pss:rhs>
                            <pss:binexp op="LE">
                                <pss:lhs>
                                    <pss:ref>
                                        <pss:path>address</pss:path>
                                    </pss:ref>
                                </pss:lhs>
                                <pss:rhs>
                                    <pss:number>0x1fff</pss:number>
                                </pss:rhs>
                            </pss:binexp>
                        </pss:rhs>
                    </pss:binexp>
                </pss:stmt>
            </pss:constraint>
            <pss:constraint>
                <pss:stmt>
                    <pss:binexp op="GE">
                        <pss:lhs>
                            <pss:ref>
                                <pss:path>address</pss:path>
                            </pss:ref>
                        </pss:lhs>
                        <pss:rhs>
                            <pss:number>0x1000</pss:number>
                        </pss:rhs>
                    </pss:binexp>
                </pss:stmt>
                <pss:stmt>
                    <pss:binexp op="LE">
                        <pss:lhs>
                            <pss:ref>
                                <pss:path>address</pss:path>
                            </pss:ref>
                        </pss:lhs>
                        <pss:rhs>
                            <pss:number>0x1fff</pss:number>
                        </pss:rhs>
                    </pss:binexp>
                </pss:stmt>
            </pss:constraint>
        </pss:struct>
    </pss:package>
</pss:model>
