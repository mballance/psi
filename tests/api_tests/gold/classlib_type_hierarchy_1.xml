<?xml version="1.0" encoding="UTF-8"?>
<pss:model 
  xmlns:pss="http://accellera.org/PSS"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://accellera.org/PSS PSSModel.xsd">
    <pss:package>
        <pss:struct name="Base">
            <pss:field name="a" qualifier="rand">
                <pss:type>
                    <pss:bit>
                        <pss:msb>
                            <pss:number>7</pss:number>
                        </pss:msb>
                        <pss:lsb>
                            <pss:number>0</pss:number>
                        </pss:lsb>
                    </pss:bit>
                </pss:type>
            </pss:field>
            <pss:field name="b" qualifier="rand">
                <pss:type>
                    <pss:bit>
                        <pss:msb>
                            <pss:number>7</pss:number>
                        </pss:msb>
                        <pss:lsb>
                            <pss:number>0</pss:number>
                        </pss:lsb>
                    </pss:bit>
                </pss:type>
            </pss:field>
            <pss:constraint name="c">
                <pss:stmt>
                    <pss:binexp op="EqEq">
                        <pss:lhs>
                            <pss:ref>
                                <pss:path>a</pss:path>
                            </pss:ref>
                        </pss:lhs>
                        <pss:rhs>
                            <pss:number>0x5</pss:number>
                        </pss:rhs>
                    </pss:binexp>
                </pss:stmt>
                <pss:stmt>
                    <pss:binexp op="EqEq">
                        <pss:lhs>
                            <pss:ref>
                                <pss:path>b</pss:path>
                            </pss:ref>
                        </pss:lhs>
                        <pss:rhs>
                            <pss:number>0x6</pss:number>
                        </pss:rhs>
                    </pss:binexp>
                </pss:stmt>
            </pss:constraint>
        </pss:struct>
        <pss:struct name="Ext">
            <pss:super>
                <pss:path>Base</pss:path>
            </pss:super>
            <pss:constraint name="c">
                <pss:stmt>
                    <pss:binexp op="EqEq">
                        <pss:lhs>
                            <pss:ref>
                                <pss:path>a</pss:path>
                            </pss:ref>
                        </pss:lhs>
                        <pss:rhs>
                            <pss:number>0x7</pss:number>
                        </pss:rhs>
                    </pss:binexp>
                </pss:stmt>
                <pss:stmt>
                    <pss:binexp op="EqEq">
                        <pss:lhs>
                            <pss:ref>
                                <pss:path>b</pss:path>
                            </pss:ref>
                        </pss:lhs>
                        <pss:rhs>
                            <pss:number>0x8</pss:number>
                        </pss:rhs>
                    </pss:binexp>
                </pss:stmt>
            </pss:constraint>
        </pss:struct>
    </pss:package>
</pss:model>
