<?xml version="1.0" encoding="UTF-8"?>
<pss:model 
  xmlns:pss="http://accellera.org/PSS"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://accellera.org/PSS PSSModel.xsd">
    <pss:package>
        <pss:struct name="R" qualifier="resource">
            <pss:field name="instance_id" qualifier="rand">
                <pss:type>
                    <pss:bit>
                        <pss:msb>
                            <pss:number>31</pss:number>
                        </pss:msb>
                        <pss:lsb>
                            <pss:number>0</pss:number>
                        </pss:lsb>
                    </pss:bit>
                </pss:type>
            </pss:field>
        </pss:struct>
    </pss:package>
    <pss:component name="C">
        <pss:action name="A">
            <pss:field name="rc" qualifier="lock">
                <pss:type>
                    <pss:user>
                        <pss:path>R</pss:path>
                    </pss:user>
                </pss:type>
            </pss:field>
        </pss:action>
    </pss:component>
    <pss:component name="static_structure">
        <pss:field name="c1">
            <pss:type>
                <pss:user>
                    <pss:path>C</pss:path>
                </pss:user>
            </pss:type>
        </pss:field>
        <pss:field name="c2">
            <pss:type>
                <pss:user>
                    <pss:path>C</pss:path>
                </pss:user>
            </pss:type>
        </pss:field>
        <pss:field name="rp" qualifier="pool">
            <pss:type>
                <pss:user>
                    <pss:path>R</pss:path>
                </pss:user>
            </pss:type>
        </pss:field>
        <pss:bind>
            <pss:bindpath>
                <pss:path>rp</pss:path>
            </pss:bindpath>
            <pss:bindpath>
                <pss:path>c1</pss:path>
            </pss:bindpath>
        </pss:bind>
        <pss:bind>
            <pss:bindpath>
                <pss:path>rp</pss:path>
            </pss:bindpath>
            <pss:bindpath>
                <pss:path>c2</pss:path>
            </pss:bindpath>
        </pss:bind>
        <pss:bind>
            <pss:bindpath>
                <pss:path>rp</pss:path>
            </pss:bindpath>
            <pss:bindpath>
                <pss:path>c1</pss:path>
            </pss:bindpath>
            <pss:bindpath>
                <pss:path>c2</pss:path>
            </pss:bindpath>
        </pss:bind>
    </pss:component>
    <pss:component name="top">
        <pss:action name="entry_point">
            <pss:field name="a1">
                <pss:type>
                    <pss:user>
                        <pss:path>C</pss:path>
                        <pss:path>A</pss:path>
                    </pss:user>
                </pss:type>
            </pss:field>
            <pss:field name="a2">
                <pss:type>
                    <pss:user>
                        <pss:path>C</pss:path>
                        <pss:path>A</pss:path>
                    </pss:user>
                </pss:type>
            </pss:field>
            <pss:graph>
                <pss:parallel>
                    <pss:production>
                        <pss:traverse name="a1"/>
                    </pss:production>
                    <pss:production>
                        <pss:traverse name="a2"/>
                    </pss:production>
                </pss:parallel>
            </pss:graph>
        </pss:action>
    </pss:component>
</pss:model>
