<?xml version="1.0" encoding="UTF-8"?>
<pss:model 
  xmlns:pss="http://accellera.org/PSS"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://accellera.org/PSS PSSModel.xsd">
    <pss:component name="top">
        <pss:action name="entry_point">
            <pss:field name="p1">
                <pss:type>
                    <pss:chandle/>
                </pss:type>
            </pss:field>
            <pss:field name="p2">
                <pss:type>
                    <pss:bit>
                        <pss:msb>
                            <pss:number>0x1f</pss:number>
                        </pss:msb>
                        <pss:lsb>
                            <pss:number>0x0</pss:number>
                        </pss:lsb>
                    </pss:bit>
                </pss:type>
            </pss:field>
            <pss:field name="p3">
                <pss:type>
                    <pss:chandle/>
                </pss:type>
            </pss:field>
            <pss:exec>
                <pss:inline kind="pre_solve" address="0xDEADBEEFDEADBEEF"/>
            </pss:exec>
            <pss:exec>
                <pss:inline kind="post_solve" address="0xDEADBEEFDEADBEEF"/>
            </pss:exec>
        </pss:action>
    </pss:component>
</pss:model>
